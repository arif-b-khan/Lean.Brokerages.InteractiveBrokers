## Sample .env for QuantConnect InteractiveBrokers ToolBox
## Copy this to .env (same directory) or elsewhere and point the helper script at it.

# Interactive Brokers credentials (do NOT commit real secrets)
IB_USERNAME=your_ib_username
IB_PASSWORD=your_ib_password
IB_ACCOUNT=U1234567

# Optional overrides
IB_GATEWAY_DIR=/Users/your-user/Jts
IB_VERSION=latest
IB_TRADING_MODE=paper
IB_AUTOMATER_EXPORT_LOGS=false
GATEWAY_HOST=127.0.0.1
GATEWAY_PORT=7497
DATA_DIR=/Users/<usersname>/github/Lean/Data
LOG_LEVEL=Information
